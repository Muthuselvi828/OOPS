package sumofnumbers;

public class Sum {
	
	void add(int a,int b)
	{
		int sum=0;
		for(int i=a;i<=b;i++) {
			sum=sum+i;
		}
		System.out.println("The Sum of "+a+ " to "+b+ " is " +sum);
	}

	public static void main(String[] args) {
		
		Sum num =new Sum();
		System.out.println("Muthuselvi");	
		System.out.println("2117240050087");
		num.add(1,1);
		
		
	}

}
